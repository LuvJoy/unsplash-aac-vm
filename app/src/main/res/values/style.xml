<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="BottomNavFontStyle">
        <item name="fontFamily">@font/sandol_regular</item>
        <item name="android:textColor">@color/mainColor</item>
    </style>

    <style name="Rounded">
        <item name="cornerSize">4%</item>
    </style>

    <style name="Circle">
        <item name="cornerSize">50%</item>
    </style>

    <style name="BottomRounded">
        <item name="cornerSizeBottomLeft">24dp</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerSizeBottomRight">24dp</item>
        <item name="cornerFamilyBottomRight">rounded</item>
    </style>

    <style name="TopRounded">
        <item name="cornerSizeTopLeft">24dp</item>
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerSizeTopRight">24dp</item>
        <item name="cornerFamilyTopRight">rounded</item>
    </style>


</resources>